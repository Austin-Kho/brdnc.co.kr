# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?brinc.co.kr$
RewriteCond %{REQUEST_URI} !^/ns/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /ns/$1
RewriteCond %{HTTP_HOST} ^(www.)?brinc.co.kr$
RewriteRule ^(/)?$ ns/index.php [L]
</IfModule>
# END WordPress

